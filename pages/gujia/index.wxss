page {
    background: #f9f9f9;
}

.body {
    width: 100%;
    margin-top: 20rpx;
    padding-bottom: 24rpx;
    background: #fff;
}

.modle {
    width: 90%;
    margin-left: 5%;
    height: 88rpx;
    font-size: 28rpx;
    color: #333333;
    line-height: 88rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.modle_right {
    color: #9295A8;
    font-size: 24rpx;
    display: block;
    height: 88rpx;
    padding: 0;
    margin: 0;
    text-align: right;
    float: right;
}

.shop_view_border {
    width: 100%;
}

.shop_view {
    width: 165rpx;
    height: 165rpx;
    display: inline-block;
    margin-left: 30rpx;
}

.shop_view_img {
    width: 165rpx;
    height: 165rpx;
}

.shop_view_closeimg {
    height: 38rpx;
    width: 38rpx;
    position: absolute;
    margin-left: 127rpx;
}

.commit_btn {
    padding: 0;
    height: 72rpx;
    line-height: 72rpx;
    width: 690rpx;
    margin: 0 auto;
    font-size: 28rpx;
    text-align: center;
    color: #FFFFFF;
    background: #ff6600;
    margin-top: 80rpx;
    border-radius: 15rpx;
}

button::after {
    border: none;
}

.shouquan_btn {
    height: 60rpx;
    margin-top: 14rpx;
    width: 150rpx;
    padding: 0;
    background: #fff;
    float: right;
    text-align: center;
    border-radius: 10rpx;
    border: 1rpx solid #ff6600;
    color: #ff6600;
    font-size: 27rpx;
    line-height: 56rpx;
}

.ad-container{
  width: 100%;
  height: 280rpx;
  margin-top: 20rpx;
}
.ad-container image{
  width: 100%;
  height: 300rpx;
}