page {
    background: #f4f4f4;
}

.modle_1_nav {
    position: fixed;
    top: 0rpx;
    width: 100%;
    height: 80rpx;
    z-index: 10;
    background: #fff;
    font-size: 0;
    border-bottom: 1rpx solid #f4f4f4;
    line-height: 80rpx;
}

.modle_1_nav_modle {
    color: #333;
    font-size: 28rpx;
    text-align: center;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: inline-block;
}

.modle_1_nav_active {
    color: #ff6600;
    line-height: 72rpx;
}

.modle_1_nav_modle_border {
    width: 30rpx;
    margin: 0 auto;
    height: 5rpx;
    background: #ff6600;
}

.nav {
    height: 80rpx;
    width: 100%;
}

.shop_border {
    background: #fff;
    padding: 30rpx 30rpx 30rpx 30rpx;
    border-bottom: 20rpx solid #f4f4f4;
}

.shop_modle {
    height: 180rpx;
    border-bottom: 1rpx solid #f4f4f4;
}

.shop_modle_img {
    position: absolute;
    height: 150rpx;
    width: 150rpx;
    border-radius: 10rpx;
}

.shop_modle_name {
    font-size: 32rpx;
    position: absolute;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-left: 170rpx;
    width: 400rpx;
    color: #333;
}

.shop_modle_daibaojia {
    font-size: 28rpx;
    color: #f75f5f;
    position: absolute;
    margin-left: 606rpx;
}

.shop_modle_orderTime {
    font-size: 24rpx;
    position: absolute;
    width: 400rpx;
    margin-top: 117rpx;
    margin-left: 170rpx;
    color: #7a7a7a;
}

.shop_modle_orderNumber {
    position: absolute;
    width: 400rpx;
    margin-top: 60rpx;
    margin-left: 170rpx;
    font-size: 24rpx;
    color: #7a7a7a;
}

.shop_modle_orderbaojia {
    color: #f75f5f;
    font-size: 28rpx;
}

.shop_modle_wancheng {
    color: #7a7a7a;
}

.shop_modle_btn {
    position: absolute;
    margin-top: 100rpx;
    margin-left: 560rpx;
    width: 130rpx;
    height: 50rpx;
    text-align: center;
    font-size: 24rpx;
    padding: 0;
    background: #fff;
    color: #f75f5f;
    line-height: 48rpx;
    border: 1rpx solid #f75f5f;
    border-radius: 6rpx;
}

button::after {
    border: none;
}

.shop_modle_btmNav {
    font-size: 24rpx;
    color: #333;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin-top: 15rpx;
}

.shop_modle_btmNav_img {
    background: #ff6600;
    height: 6rpx;
    width: 6rpx;
    border-radius: 100%;
    vertical-align: middle;
    display: inline-block;
    margin-top: -5rpx;
    margin-right: 15rpx;
}

.end_none {
    width: 100%;
    font-size: 28rpx;
    color: #8a8a8a;
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
}