page {
    height: 100%;
}

.modle_border {
    width: 100%;
    background: #fff;
}

.modle {
    width: 90%;
    margin-left: 5%;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    height: 180rpx;
    border-bottom: 1rpx solid #f0f0f0;
}

.modle_img {
    height: 180rpx;
    width: 180rpx;
    float: left;
}

.modle_name {
    font-size: 30rpx;
    color: #333333;
    line-height: 50rpx;
    padding-left: 25rpx;
    width: 470rpx;
    float: left;
}

.modle_chexing {
    font-size: 26rpx;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #555555;
    padding-left: 25rpx;
    line-height: 54rpx;
    width: 470rpx;
    float: left;
    min-height: 54rpx;
}

.modle_chexing_btn {
    background: #ff6600;
    width: 72rpx;
    height: 40rpx;
    text-align: center;
    line-height: 40rpx;
    float: right;
    font-size: 20rpx;
    color: #FFFFFF;
    border-radius: 10rpx;
}

.modle_price {
    font-size: 30rpx;
    color: #F75F5F;
    margin-top: 35rpx;
    font-weight: bold;
    line-height: 40rpx;
    padding-left: 20rpx;
    width: 475rpx;
    float: left;
}

.modle_time {
    font-size: 24rpx;
    color: #9295A8;
    margin-top: 15rpx;
    line-height: 40rpx;
    padding-left: 20rpx;
    width: 475rpx;
    float: left;
}

.modle_btn_view {
    font-size: 24rpx;
    color: #F75F5F;
    margin-top: 40rpx;
    line-height: 40rpx;
    padding-left: 20rpx;
    width: 475rpx;
    float: left;
}

.modle_chexing_btnsc {
    padding: 0;
    margin: 0;
    background: #fff;
    width: 72rpx;
    height: 38rpx;
    text-align: center;
    line-height: 38rpx;
    float: right;
    border: 1px solid #F75F5F;
    font-size: 25rpx;
    color: #F75F5F;
    border-radius: 5rpx;
}

button::after {
    border: none;
}

.modle_name_left {
    width: 330rpx;
    word-break: keep-all;
    white-space: nowrap;
    font-weight: bold;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
}

.modle_name_right {
    font-weight: bold;
    float: right;
    font-size: 30rpx;
    color: #F75F5F;
}

.modle_name_right_active {
    color: #F75F5F;
    font-weight: bold;
    line-height: 40rpx;
}

.btn_active {
    color: #333333;
    border: 1rpx solid #333333;
}

.modle_1_nav {
    position: fixed;
    z-index: 100;
    top: 0rpx;
    width: 100%;
    height: 70rpx;
    color: #666666;
    font-size: 28rpx;
    border-bottom: 1rpx solid #f9f9f9;
    line-height: 70rpx;
    background-color: #fff;
}

.modle_1_nav_modle {
    text-align: center;
    word-break: keep-all;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    display: inline-block;
}

.modle_1_nav_modle_border {
    width: 30rpx;
    margin: 0 auto;
    height: 5rpx;
    background: #333;
}

.modle_1_nav_active {
    color: #333;
    line-height: 62rpx;
}

.nav {
    height: 70rpx;
    width: 100%;
}