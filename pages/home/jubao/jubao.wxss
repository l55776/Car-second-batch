@import "../../alert/index.wxss";

.banner {
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    width: 90%;
    padding-left: 5%;
    padding-right: 5%;
}

.banner_top {
    font-size: 26rpx;
    color: #737985;
    width: 100%;
}

.banner_top_img {
    height: 28rpx;
    vertical-align: middle;
    margin-top: -5rpx;
    padding-right: 5rpx;
    width: 28rpx;
}

.banner_bottom {
    font-size: 26rpx;
    color: #737985;
    margin-top: 16rpx;
    width: 100%;
}

.modle_nav {
    width: 90%;
    margin-left: 5%;
    margin-right: 5%;
    border-bottom: 1rpx solid #f9f9f9;
    line-height: 87rpx;
    padding-top: 1rpx;
    height: 87rpx;
    font-size: 30rpx;
    color: #333333;
}

.modle_nav_biaoji {
    width: 6rpx;
    display: inline-block;
    border-right: 16rpx solid #fff;
    height: 24rpx;
    margin-top: 32rpx;
    background: #ff6600;
}

.nav {
    width: 100%;
    height: 20rpx;
    background: #f9f9f9;
}

radio .wx-radio-input {
    border-radius: 50%;
    height: 35rpx;
    width: 35rpx;
}

radio .wx-radio-input.wx-radio-input-checked {
    border: none;
    background: red;
}

radio .wx-radio-input.wx-radio-input-checked::before {
    border-radius: 50%;
    width: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    font-size: 25rpx;
    color: #fff;
    background: transparent;
    transform: translate(-50%,-50%) scale(1);
}

.text_area {
    width: 90%;
    padding-left: 5%;
    padding-right: 5%;
    height: 270rpx;
    margin-top: 30rpx;
    font-size: 24rpx;
    color: #333;
}

.btn_view {
    height: 140rpx;
    margin-top: 50rpx;
    width: 100%;
    background: #fff;
    border-top: 1rpx solid #f9f9f9;
    bottom: 0rpx;
}

.tijiao_btn {
    padding: 0;
    margin: 0;
    text-align: center;
    bottom: 30rpx;
    width: 90%;
    margin-left: 5%;
    line-height: 80rpx;
    background: #B2B2B2;
    border-radius: 20rpx;
    font-size: 28rpx;
    color: #FFFFFF;
    height: 80rpx;
}

.chooseType_view {
    width: 100%;
    height: 100%;
    z-index: 25;
    overflow-y: scroll;
    position: fixed;
    top: 0rpx;
    left: 0rpx;
    background: #fff;
}

button::after {
    border: none;
}

.tijiao_btn_active {
    background: #ff6600;
}

.modle {
    width: 690rpx;
    margin: 0 auto;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    border-bottom: 1rpx solid #f9f9f9;
}

.modle_top {
    width: 100%;
    height: 72rpx;
}

.modle_top_img {
    height: 72rpx;
    width: 72rpx;
    border-radius: 100%;
    float: left;
}

.modle_top_text_index {
    font-size: 28rpx;
    margin-left: 15rpx;
    color: #333333;
    width: 400rpx;
    float: left;
}

.modle_top_text_second {
    font-size: 20rpx;
    color: #9195A3;
    margin-top: 3rpx;
    width: 400rpx;
    margin-left: 15rpx;
    float: left;
}

.modle_text {
